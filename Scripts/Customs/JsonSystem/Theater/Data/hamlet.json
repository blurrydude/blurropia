                { "TriggerTime": "00:11:25", "Actor":3 , "Action": "say", "Data": null, "Text": "It faded on the crowing of the cock." },
                { "TriggerTime": "00:11:30", "Actor":3 , "Action": "say", "Data": null, "Text": "Some say that ever 'gainst that season comes" },
                { "TriggerTime": "00:11:35", "Actor":3 , "Action": "say", "Data": null, "Text": "Wherein our Saviour's birth is celebrated," },
