                { "TriggerTime": "00:12:05", "Actor":2 , "Action": "say", "Data": null, "Text": "So have I heard and do in part believe it." },
                { "TriggerTime": "00:12:10", "Actor":2 , "Action": "say", "Data": null, "Text": "But, look, the morn, in russet mantle clad," },
