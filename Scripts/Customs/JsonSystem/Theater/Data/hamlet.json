                { "TriggerTime": "00:09:40", "Actor":2 , "Action": "say", "Data": null, "Text": "For which, they say, you spirits oft walk in death," },
                { "TriggerTime": "00:09:45", "Actor":2 , "Action": "say", "Data": null, "Text": "Speak of it: stay, and speak! Stop it, Marcellus." },
                { "TriggerTime": "00:09:50", "Actor":3 , "Action": "say", "Data": null, "Text": "Shall I strike at it with my partisan?" },
                { "TriggerTime": "00:09:55", "Actor":2 , "Action": "say", "Data": null, "Text": "Do, if it will not stand." },
                { "TriggerTime": "00:10:00", "Actor":0 , "Action": "say", "Data": null, "Text": "'Tis here!" },
                { "TriggerTime": "00:10:05", "Actor":2 , "Action": "say", "Data": null, "Text": "'Tis here!" },
