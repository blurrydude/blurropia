                { "TriggerTime": "00:09:15", "Actor":2 , "Action": "say", "Data": null, "Text": "Speak to me:" },
            
				{ "TriggerTime": "00:09:20", "Actor":2 , "Action": "say", "Data": null, "Text": "If thou art privy to thy country's fate," },
                { "TriggerTime": "00:09:25", "Actor":2 , "Action": "say", "Data": null, "Text": "Which, happily, foreknowing may avoid, O, speak!" },
                { "TriggerTime": "00:09:30", "Actor":2 , "Action": "say", "Data": null, "Text": "Or if thou hast uphoarded in thy life" },
