                { "TriggerTime": "00:08:20", "Actor":2 , "Action": "say", "Data": null, "Text": "And even the like precurse of fierce events," },
                { "TriggerTime": "00:08:25", "Actor":2 , "Action": "say", "Data": null, "Text": "As harbingers preceding still the fates" },
                { "TriggerTime": "00:08:30", "Actor":2 , "Action": "say", "Data": null, "Text": "And prologue to the omen coming on," },
