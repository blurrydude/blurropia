			"Actions": [
				{ "TriggerTime": "00:00:01", "Actor": 1, "Action": "face", "Data": null, "Text": "South" },
				{ "TriggerTime": "00:00:01", "Actor": 0, "Action": "walk", "Data": [0, 2], "Text": null },
                { "TriggerTime": "00:00:03", "Actor": 0, "Action": "walk", "Data": [5, 2], "Text": null },
                { "TriggerTime": "00:00:05", "Actor": 1, "Action": "face", "Data": null, "Text": "West" },
				{ "TriggerTime": "00:00:05", "Actor": 0, "Action": "say", "Data": null, "Text": "Who's there?" },
                { "TriggerTime": "00:00:07", "Actor": 0, "Action": "face", "Data": null, "Text": "East" },
				{ "TriggerTime": "00:00:07", "Actor": 1, "Action": "say", "Data": null, "Text": "Nay, answer me: stand, and unfold yourself." },
                { "TriggerTime": "00:00:10", "Actor": 0, "Action": "face", "Data": null, "Text": "East" },
				{ "TriggerTime": "00:00:12", "Actor": 0, "Action": "say", "Data": null, "Text": "Long live the King!" },
				{ "TriggerTime": "00:00:16", "Actor": 1, "Action": "say", "Data": null, "Text": "Bernardo?" },
				{ "TriggerTime": "00:00:17", "Actor": 0, "Action": "say", "Data": null, "Text": "He." },
                { "TriggerTime": "00:00:17", "Actor": 0, "Action": "walk", "Data": [9, 2], "Text": null },
                { "TriggerTime": "00:00:20", "Actor": 0, "Action": "face", "Data": null, "Text": "East" },
				{ "TriggerTime": "00:00:19", "Actor": 1, "Action": "say", "Data": null, "Text": "You come most carefully upon your hour." },
				{ "TriggerTime": "00:00:23", "Actor": 0, "Action": "say", "Data": null, "Text": "'Tis now struck twelve; get thee to bed, Francisco." },
				{ "TriggerTime": "00:00:27", "Actor": 1, "Action": "say", "Data": null, "Text": "For this relief much thanks: 'tis bitter cold, and I am sick at heart." },
				{ "TriggerTime": "00:00:31", "Actor": 0, "Action": "say", "Data": null, "Text": "Have you had quiet guard?" },
				{ "TriggerTime": "00:00:34", "Actor": 1, "Action": "say", "Data": null, "Text": "Not a mouse stirring." },
				{ "TriggerTime": "00:00:37", "Actor": 0, "Action": "say", "Data": null, "Text": "Well, good night." },
                { "TriggerTime": "00:00:40", "Actor": 0, "Action": "say", "Data": null, "Text": "If you do meet Horatio and Marcellus, the rivals of my watch, bid them make haste." },
				{ "TriggerTime": "00:00:35", "Actor": 2, "Action": "walk", "Data": [0, 3], "Text": null },
                { "TriggerTime": "00:00:40", "Actor": 2, "Action": "walk", "Data": [7, 3], "Text": null },
                { "TriggerTime": "00:00:36", "Actor": 3, "Action": "walk", "Data": [0, 1], "Text": null },
                { "TriggerTime": "00:00:42", "Actor": 3, "Action": "walk", "Data": [6, 1], "Text": null },
				{ "TriggerTime": "00:00:45", "Actor": 1, "Action": "say", "Data": null, "Text": "I think I hear them. Stand, ho! Who's there?" },
                { "TriggerTime": "00:00:40", "Actor": 2, "Action": "face", "Data": null, "Text": "East" },
                { "TriggerTime": "00:00:41", "Actor": 3, "Action": "face", "Data": null, "Text": "East" },
				{ "TriggerTime": "00:00:48", "Actor": 2, "Action": "say", "Data": null, "Text": "Friends to this ground." },
				{ "TriggerTime": "00:00:50", "Actor": 3, "Action": "say", "Data": null, "Text": "And liegemen to the Dane." },
				{ "TriggerTime": "00:00:53", "Actor": 1, "Action": "say", "Data": null, "Text": "Give you good night." },
				{ "TriggerTime": "00:00:56", "Actor": 3, "Action": "say", "Data": null, "Text": "O, farewell, honest soldier: who hath relieved you?" },
				{ "TriggerTime": "00:01:00", "Actor": 1, "Action": "say", "Data": null, "Text": "Bernardo has my place." },
                { "TriggerTime": "00:01:04", "Actor": 1, "Action": "say", "Data": null, "Text": "Give you good night." },
                
                { "TriggerTime": "00:01:04", "Actor": 1, "Action": "walk", "Data": [19, 3], "Text": null },
                { "TriggerTime": "00:01:08", "Actor": 1, "Action": "walk", "Data": [19, 6], "Text": null },
                { "TriggerTime": "00:01:05", "Actor": 0, "Action": "walk", "Data": [12, 3], "Text": null },
                { "TriggerTime": "00:01:07", "Actor": 0, "Action": "face", "Data": null, "Text": "West" },
                { "TriggerTime": "00:01:10", "Actor": 0, "Action": "face", "Data": null, "Text": "West" },
                { "TriggerTime": "00:01:11", "Actor": 3, "Action": "say", "Data": null, "Text": "Holla! Bernardo!" },
                { "TriggerTime": "00:01:14", "Actor": 0, "Action": "say", "Data": null, "Text": "Say, what, is Horatio there?" },
                { "TriggerTime": "00:01:17", "Actor": 2, "Action": "say", "Data": null, "Text": "A piece of him." },
                { "TriggerTime": "00:01:20", "Actor": 0, "Action": "say", "Data": null, "Text": "Welcome, Horatio: welcome, good Marcellus." },
                { "TriggerTime": "00:01:24", "Actor": 3, "Action": "say", "Data": null, "Text": "What, has this thing appear'd again to-night?" },
                { "TriggerTime": "00:01:28", "Actor": 0, "Action": "say", "Data": null, "Text": "I have seen nothing." },
                { "TriggerTime": "00:01:31", "Actor": 3, "Action": "say", "Data": null, "Text": "Horatio says 'tis but our fantasy," },
                { "TriggerTime": "00:01:35", "Actor": 3, "Action": "say", "Data": null, "Text": "And will not let belief take hold of him" },
                { "TriggerTime": "00:01:39", "Actor": 3, "Action": "say", "Data": null, "Text": "Touching this dreaded sight, twice seen of us:" },
                { "TriggerTime": "00:01:43", "Actor": 3, "Action": "say", "Data": null, "Text": "Therefore I have entreated him along" },
                { "TriggerTime": "00:01:47", "Actor": 3, "Action": "say", "Data": null, "Text": "With us to watch the minutes of this night;" },
                { "TriggerTime": "00:01:51", "Actor": 3, "Action": "say", "Data": null, "Text": "That if again this apparition come," },
                { "TriggerTime": "00:01:55", "Actor": 3, "Action": "say", "Data": null, "Text": "He may approve our eyes and speak to it." },
                { "TriggerTime": "00:01:59", "Actor": 2, "Action": "say", "Data": null, "Text": "Tush, tush, 'twill not appear." },
                { "TriggerTime": "00:02:02", "Actor": 0, "Action": "say", "Data": null, "Text": "Sit down awhile;" },
                { "TriggerTime": "00:02:04", "Actor": 0, "Action": "say", "Data": null, "Text": "And let us once again assail your ears," },
                { "TriggerTime": "00:02:07", "Actor": 0, "Action": "say", "Data": null, "Text": "That are so fortified against our story" },
                { "TriggerTime": "00:02:10", "Actor": 0, "Action": "say", "Data": null, "Text": "What we have two nights seen." },
                { "TriggerTime": "00:02:13", "Actor": 2, "Action": "say", "Data": null, "Text": "Well, sit we down," },
                { "TriggerTime": "00:02:16", "Actor": 2, "Action": "say", "Data": null, "Text": "And let us hear Bernardo speak of this." },
                { "TriggerTime": "00:02:20", "Actor": 0, "Action": "say", "Data": null, "Text": "Last night of all," },
                { "TriggerTime": "00:02:23", "Actor": 0, "Action": "say", "Data": null, "Text": "When yond same star that's westward from the pole" },
                { "TriggerTime": "00:02:27", "Actor": 0, "Action": "say", "Data": null, "Text": "Had made his course to illume that part of heaven" },
                { "TriggerTime": "00:02:32", "Actor": 0, "Action": "say", "Data": null, "Text": "Where now it burns, Marcellus and myself," },
                { "TriggerTime": "00:02:36", "Actor": 0, "Action": "say", "Data": null, "Text": "The bell then beating one" },
                { "TriggerTime": "00:02:34", "Actor":4 , "Action": "walk", "Data": [0, 0], "Text": null },
                { "TriggerTime": "00:02:40", "Actor": 3, "Action": "say", "Data": null, "Text": "Peace, break thee off; look, where it comes again!" },
                { "TriggerTime": "00:02:45", "Actor": 0, "Action": "say", "Data": null, "Text": "In the same figure, like the king that's dead." },
                { "TriggerTime": "00:02:50", "Actor": 3, "Action": "say", "Data": null, "Text": "Thou art a scholar; speak to it, Horatio." },
                { "TriggerTime": "00:02:55", "Actor": 0, "Action": "say", "Data": null, "Text": "Looks it not like the king? mark it, Horatio." },
                { "TriggerTime": "00:03:00", "Actor": 2, "Action": "say", "Data": null, "Text": "Most like: it harrows me with fear and wonder." },
                { "TriggerTime": "00:03:04", "Actor": 0, "Action": "say", "Data": null, "Text": "It would be spoke to." },
                { "TriggerTime": "00:03:06", "Actor": 3, "Action": "say", "Data": null, "Text": "Question it, Horatio." },
                { "TriggerTime": "00:03:10", "Actor": 2, "Action": "say", "Data": null, "Text": "What art thou that usurp'st this time of night," },
                { "TriggerTime": "00:03:14", "Actor": 2, "Action": "say", "Data": null, "Text": "Together with that fair and warlike form" },
                { "TriggerTime": "00:03:18", "Actor": 2, "Action": "say", "Data": null, "Text": "In which the majesty of buried Denmark" },
                { "TriggerTime": "00:03:22", "Actor": 2, "Action": "say", "Data": null, "Text": "Did sometimes march? by heaven I charge thee, speak!" },
                { "TriggerTime": "00:03:26", "Actor": 3, "Action": "say", "Data": null, "Text": "It is offended." },
                { "TriggerTime": "00:03:31", "Actor": 2, "Action": "say", "Data": null, "Text": "Stay! speak, speak! I charge thee, speak!" },
                { "TriggerTime": "00:03:22", "Actor":4 , "Action": "walk", "Data": [0, 6], "Text": null },
                { "TriggerTime": "00:03:35", "Actor":3 , "Action": "say", "Data": null, "Text": "'Tis gone, and will not answer." },
                { "TriggerTime": "00:03:40", "Actor":0 , "Action": "say", "Data": null, "Text": "How now, Horatio! you tremble and look pale:" },
                { "TriggerTime": "00:03:45", "Actor":0 , "Action": "say", "Data": null, "Text": "Is not this something more than fantasy?" },
                { "TriggerTime": "00:03:50", "Actor":0 , "Action": "say", "Data": null, "Text": "What think you on't?" },
                { "TriggerTime": "00:03:55", "Actor":2 , "Action": "say", "Data": null, "Text": "Before my God, I might not this believe" },
                { "TriggerTime": "00:04:00", "Actor":2 , "Action": "say", "Data": null, "Text": "Without the sensible and true avouch" },
                { "TriggerTime": "00:04:05", "Actor":2 , "Action": "say", "Data": null, "Text": "Of mine own eyes." },
                { "TriggerTime": "00:04:10", "Actor":3 , "Action": "say", "Data": null, "Text": "Is it not like the king?" },
                { "TriggerTime": "00:04:15", "Actor":2 , "Action": "say", "Data": null, "Text": "As thou art to thyself:" },
                { "TriggerTime": "00:04:20", "Actor":2 , "Action": "say", "Data": null, "Text": "Such was the very armour he had on" },
                { "TriggerTime": "00:04:25", "Actor":2 , "Action": "say", "Data": null, "Text": "When he the ambitious Norway combated;" },
                { "TriggerTime": "00:04:30", "Actor":2 , "Action": "say", "Data": null, "Text": "So frown'd he once, when, in an angry parle," },
                { "TriggerTime": "00:04:35", "Actor":2 , "Action": "say", "Data": null, "Text": "He smote the sledded Polacks on the ice." },
                { "TriggerTime": "00:04:40", "Actor":2 , "Action": "say", "Data": null, "Text": "'Tis strange." },
                { "TriggerTime": "00:04:45", "Actor":3 , "Action": "say", "Data": null, "Text": "Thus twice before, and jump at this dead hour," },
                { "TriggerTime": "00:04:50", "Actor":3 , "Action": "say", "Data": null, "Text": "With martial stalk hath he gone by our watch." },
                { "TriggerTime": "00:04:55", "Actor":2 , "Action": "say", "Data": null, "Text": "In what particular thought to work I know not;" },
                { "TriggerTime": "00:05:00", "Actor":2 , "Action": "say", "Data": null, "Text": "But in the gross and scope of my opinion," },
                { "TriggerTime": "00:05:05", "Actor":2 , "Action": "say", "Data": null, "Text": "This bodes some strange eruption to our state." },
                { "TriggerTime": "00:05:00", "Actor":3 , "Action": "say", "Data": null, "Text": "Good now, sit down, and tell me, he that knows," },
                { "TriggerTime": "00:05:05", "Actor":3 , "Action": "say", "Data": null, "Text": "Why this same strict and most observant watch" },
                { "TriggerTime": "00:05:00", "Actor":3 , "Action": "say", "Data": null, "Text": "So nightly toils the subject of the land," },
                { "TriggerTime": "00:05:05", "Actor":3 , "Action": "say", "Data": null, "Text": "And why such daily cast of brazen cannon," },
                { "TriggerTime": "00:05:00", "Actor":3 , "Action": "say", "Data": null, "Text": "And foreign mart for implements of war;" },
                { "TriggerTime": "00:05:05", "Actor":3 , "Action": "say", "Data": null, "Text": "Why such impress of shipwrights, whose sore task" },
                { "TriggerTime": "00:05:00", "Actor":3 , "Action": "say", "Data": null, "Text": "Does not divide the Sunday from the week;" },
                { "TriggerTime": "00:05:05", "Actor":3 , "Action": "say", "Data": null, "Text": "What might be toward, that this sweaty haste" },
                { "TriggerTime": "00:05:00", "Actor":3 , "Action": "say", "Data": null, "Text": "Doth make the night joint-labourer with the day:" },
                { "TriggerTime": "00:05:05", "Actor":3 , "Action": "say", "Data": null, "Text": "Who is't that can inform me?" },
                { "TriggerTime": "00:05:00", "Actor":2 , "Action": "say", "Data": null, "Text": "That can I;" },
                { "TriggerTime": "00:05:05", "Actor":2 , "Action": "say", "Data": null, "Text": "At least, the whisper goes so. Our last king," },
                { "TriggerTime": "00:05:00", "Actor":2 , "Action": "say", "Data": null, "Text": "Whose image even but now appear'd to us," },
                { "TriggerTime": "00:05:05", "Actor":2 , "Action": "say", "Data": null, "Text": "Was, as you know, by Fortinbras of Norway," },
                { "TriggerTime": "00:05:10", "Actor":2 , "Action": "say", "Data": null, "Text": "Thereto prick'd on by a most emulate pride," },
                { "TriggerTime": "00:05:15", "Actor":2 , "Action": "say", "Data": null, "Text": "Dared to the combat; in which our valiant Hamlet--" },
                { "TriggerTime": "00:05:20", "Actor":2 , "Action": "say", "Data": null, "Text": "For so this side of our known world esteem'd him--" },
                { "TriggerTime": "00:05:25", "Actor":2 , "Action": "say", "Data": null, "Text": "Did slay this Fortinbras; who by a seal'd compact," },
                { "TriggerTime": "00:05:30", "Actor":2 , "Action": "say", "Data": null, "Text": "Well ratified by law and heraldry," },
                { "TriggerTime": "00:05:35", "Actor":2 , "Action": "say", "Data": null, "Text": "Did forfeit, with his life, all those his lands" },
                { "TriggerTime": "00:05:40", "Actor":2 , "Action": "say", "Data": null, "Text": "Which he stood seized of, to the conqueror:" },
                { "TriggerTime": "00:05:45", "Actor":2 , "Action": "say", "Data": null, "Text": "Against the which, a moiety competent" },
                { "TriggerTime": "00:05:50", "Actor":2 , "Action": "say", "Data": null, "Text": "Was gaged by our king; which had return'd" },
                { "TriggerTime": "00:05:55", "Actor":2 , "Action": "say", "Data": null, "Text": "To the inheritance of Fortinbras," },
                { "TriggerTime": "00:06:00", "Actor":2 , "Action": "say", "Data": null, "Text": "Had he been vanquisher; as, by the same covenant," },
                { "TriggerTime": "00:06:05", "Actor":2 , "Action": "say", "Data": null, "Text": "And carriage of the article design'd," },
                { "TriggerTime": "00:06:10", "Actor":2 , "Action": "say", "Data": null, "Text": "His fell to Hamlet. Now, sir, young Fortinbras," },
                { "TriggerTime": "00:06:15", "Actor":2 , "Action": "say", "Data": null, "Text": "Of unimproved mettle hot and full," },
                { "TriggerTime": "00:06:20", "Actor":2 , "Action": "say", "Data": null, "Text": "Hath in the skirts of Norway here and there" },
                { "TriggerTime": "00:06:25", "Actor":2 , "Action": "say", "Data": null, "Text": "Shark'd up a list of lawless resolutes," },
                { "TriggerTime": "00:06:30", "Actor":2 , "Action": "say", "Data": null, "Text": "For food and diet, to some enterprise" },
                { "TriggerTime": "00:06:35", "Actor":2 , "Action": "say", "Data": null, "Text": "That hath a stomach in't; which is no other--" },
                { "TriggerTime": "00:06:40", "Actor":2 , "Action": "say", "Data": null, "Text": "As it doth well appear unto our state--" },
                { "TriggerTime": "00:06:45", "Actor":2 , "Action": "say", "Data": null, "Text": "But to recover of us, by strong hand" },
                { "TriggerTime": "00:06:50", "Actor":2 , "Action": "say", "Data": null, "Text": "And terms compulsatory, those foresaid lands" },
                { "TriggerTime": "00:06:55", "Actor":2 , "Action": "say", "Data": null, "Text": "So by his father lost: and this, I take it," },
                { "TriggerTime": "00:07:00", "Actor":2 , "Action": "say", "Data": null, "Text": "Is the main motive of our preparations," },
                { "TriggerTime": "00:07:05", "Actor":2 , "Action": "say", "Data": null, "Text": "The source of this our watch and the chief head" },
                { "TriggerTime": "00:07:10", "Actor":2 , "Action": "say", "Data": null, "Text": "Of this post-haste and romage in the land." },
                { "TriggerTime": "00:07:15", "Actor":0 , "Action": "say", "Data": null, "Text": "I think it be no other but e'en so:" },
                { "TriggerTime": "00:07:20", "Actor":0 , "Action": "say", "Data": null, "Text": "Well may it sort that this portentous figure" },
                { "TriggerTime": "00:07:25", "Actor":0 , "Action": "say", "Data": null, "Text": "Comes armed through our watch; so like the king" },
                { "TriggerTime": "00:07:30", "Actor":0 , "Action": "say", "Data": null, "Text": "That was and is the question of these wars." },
                { "TriggerTime": "00:07:35", "Actor":2 , "Action": "say", "Data": null, "Text": "A mote it is to trouble the mind's eye." },
                { "TriggerTime": "00:07:40", "Actor":2 , "Action": "say", "Data": null, "Text": "In the most high and palmy state of Rome," },
                { "TriggerTime": "00:07:45", "Actor":2 , "Action": "say", "Data": null, "Text": "A little ere the mightiest Julius fell," },
                { "TriggerTime": "00:07:50", "Actor":2 , "Action": "say", "Data": null, "Text": "The graves stood tenantless and the sheeted dead" },
                { "TriggerTime": "00:07:55", "Actor":2 , "Action": "say", "Data": null, "Text": "Did squeak and gibber in the Roman streets:" },
                { "TriggerTime": "00:08:00", "Actor":2 , "Action": "say", "Data": null, "Text": "As stars with trains of fire and dews of blood," },
                { "TriggerTime": "00:08:05", "Actor":2 , "Action": "say", "Data": null, "Text": "Disasters in the sun; and the moist star" },
                { "TriggerTime": "00:08:10", "Actor":2 , "Action": "say", "Data": null, "Text": "Upon whose influence Neptune's empire stands" },
